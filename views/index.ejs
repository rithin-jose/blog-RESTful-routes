<%- include("partials/header")%>
<h1>Index</h1>

<%blogs.forEach(function(blog){%>
    <div>
        <h2><%= blog.title%></h2>
        <img src=<%= blog.image%> alt="blog photo" height="300px">
        <span><%= blog.created %></span>
        <p><%= blog.body %></p>
        <a href="/blogs/<%= blog._id %>">Read more</a>
    </div>
<%});%>
<%- include("partials/footer")%>